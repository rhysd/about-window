{
  "name": "electron-about-window",
  "version": "1.4.0",
  "description": "'About App' window for Electron application",
  "scripts": {
    "dep": "npm install && typings install",
    "build-main": "tsc --sourceMap --module commonjs $(npm prefix)/src/index.ts --outDir .",
    "build-renderer": "tsc --sourceMap --module commonjs $(npm prefix)/src/renderer.ts --outDir .",
    "build": "npm run build-main && npm run build-renderer",
    "example": "electron $(npm prefix)/example",
    "start": "npm run dep && npm run build && npm run example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/electron-about-window.git"
  },
  "keywords": [
    "Electron",
    "electron-component",
    "about this app"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/electron-about-window/issues"
  },
  "homepage": "https://github.com/rhysd/electron-about-window#readme",
  "devDependencies": {
    "typescript": "^1.7.5",
    "typings": "^0.6.3"
  },
  "dependencies": {
    "object-assign": "^4.0.1"
  }
}
